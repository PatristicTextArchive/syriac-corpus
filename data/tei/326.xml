        <body>
            <div type="title" n="72">
                <ab xml:lang="en">Hymn LXXII</ab>
            </div>
            <div type="body">
                <lg type="superscript">
                    <l xml:lang="syr">ܒ ܡܢܗ ܒܪ ܩܠܗ </l>
                </lg>
                <lg type="stanza" n="1"> 
                    <l xml:lang="syr">ܫܡܥܘ ܘܐܬܒܝܐܘ ܡ̈ܝܘܬܐ </l>
                    <l xml:lang="syr">ܕܗܘܐ ܢܘܢܐ ܪܒ ܒܝܬܐ </l>
                    <l xml:lang="syr">ܢܣܼܒ ܘܢܼܛܪ ܘܦܿܢܝ ܘܝܗܒ </l>
                    <l xml:lang="syr">ܗܟܢ ܝܗܿܒ ܚܘܫܒܢܐ </l>
                    <l xml:lang="syr">ܡܘܬܐ ܪܒ ܒܝܬܐ ܕܫܝܘܠ </l>
                    <l xml:lang="syr">ܕܟܕ ܐܚܝ ܡܪܢ ܡ̈ܝܬܐ </l>
                    <l xml:lang="syr">ܐܦ ܬܡܪ̈ܐ ܕܥ̈ܝܢܐ ܬܒܥܗ </l>
                    
                </lg>
                
                <lg type="refrain">
                    <l xml:lang="syr">ܥܘܢܝܬܐ : ܠܟ ܬܫܒܘܚܬܐ ܡܿܠܟܐ ܡܫܝܚܐ </l>
                    <l xml:lang="syr">ܡܢܕ ܕܡ̈ܟܐ ܒܩܠ ܫܝܦܘܪܗ </l>
                    
                </lg>
                <lg type="stanza" n="2"> 
                    <l xml:lang="syr">ܐܢܗܼܘ ܕܨܦܘܪܐ ܓܙܪܬ </l>
                    <l xml:lang="syr">ܒܓܙܘܪܬܐ ܕܙܒܢܐ ܠܒܪܗܿ</l>
                    <l xml:lang="syr">ܘܒܼܛܠ ܡܘܬܐ ܒܓܙܘܪܬܐ </l>
                    <l xml:lang="syr">ܟܡܐ ܝܬܝܪ ܒܿܛܠ ܡܘܬܐ </l>
                    <l xml:lang="syr">ܒܡܥܡܘܕܝܬܐ ܕܫܪܪܐ </l>
                    <l xml:lang="syr">ܕܐܝܢܐ ܕܥܡܕ ܒܡܫܝܚܐ </l>
                    <l xml:lang="syr">ܠܒܫܗ ܠܚܝܐ ܡܚܐ ܟܠ</l>
                    
                </lg>
                <lg type="stanza" n="3"> 
                    <l>܀܀܀</l>
                    <l xml:lang="syr">ܐܝܟܢ ܛܿܠܡ ܐܠܗܐ </l>
                    <l xml:lang="syr">ܠܐܒܪܗܡ ܒܬܪ ܡܘܬܗ </l>
                    <l xml:lang="syr">ܐܘ ܚܘܒܗ ܐܘ ܙܕܩ̈ܬܗ </l>
                    <l xml:lang="syr">ܘܐܝܟܢ ܛܿܠܡ ܠܗ ܠܐܝܣܚܩ</l>
                    <l xml:lang="syr">ܦܟܪ̈ܐ ܕܐ̈ܝܕܘܗܝ ܘܕܪ̈ܓܠܘܗܝ </l>
                    <l xml:lang="syr">ܘܣܟܝܢܐ ܕܚܙܬ ܥܝܢܗ</l>
                    <l xml:lang="syr">ܕܥܠܘܗܝ ܡܢ ܫܠܝ ܐܫܬܡܛܬ</l>
                    
                </lg>
                <lg type="stanza" n="4"> 
                    <l xml:lang="syr">ܐܝܟܢ ܛܿܠܡ ܠܢܒ̈ܝܐ </l>
                    <l xml:lang="syr">ܕܐܝܟ ܐܡܪ̈ܐ ܓܙܪ̈ܝܢ ܓܙܪ̈ܝܢ </l>
                    <l xml:lang="syr">ܐܬܚܪܫܘ ܒܛܝܪ̈ܝܗܘܢ </l>
                    <l xml:lang="syr">ܕܐܝܙܒܠ ܕܐܒܬܐ ܕܢܒ̈ܝܐ</l>
                    <l xml:lang="syr">ܚܪܫܬ ܐܢܘܢ ܠܢܒ̈ܝܐ </l>
                    <l xml:lang="syr">ܘܡܐܐ ܢܒ̈ܝܝܢ ܕܐܬܿܦܠܛܘ </l>
                    <l xml:lang="syr">ܒܟܦܢܐ ܘܨܗܝܐ ܐܫܿܬܢܩܘ </l>
                    
                </lg>
                <lg type="stanza" n="5"> 
                    <l xml:lang="syr">ܐܦ ܠܠܘܛ ܐܝܟܢ ܛܿܠܡ </l>
                    <l xml:lang="syr">ܕܬܪ̈ܬܝܗܝܢ ܒ̈ܢܬܗ ܝܗܒ ܗܘܐ </l>
                    <l xml:lang="syr">ܚܠܦ ܩܕܝ̈ܫܐ ܕܥܠܘ ܨܐܕܘܗܝ </l>
                    <l xml:lang="syr">ܡܢ ܦܘܡܐ ܕܢܘܪܐ ܦܪܩܗ</l>
                    <l xml:lang="syr">ܘܒܦܘܡܐ ܕܫܝܘܠ ܫܕܝܗܝ </l>
                    <l xml:lang="syr">ܘܡܢܐ ܡܘܬܪ ܦܘܨܝܐ </l>
                    <l xml:lang="syr">ܐܢ ܢܘܚܡܐ ܠܐ ܗܿܘܐ </l>
                    
                </lg>
                <lg type="stanza" n="6"> 
                    <l xml:lang="syr">ܗܐ ܚܼܙܝ ܥܘ̈ܠܐ ܕܚܢܝܩܝܢ </l>
                    <l xml:lang="syr">ܘܢܒ̈ܝܐ ܬܘܒ ܕܢܟܝܣܝܢ </l>
                    <l xml:lang="syr">ܘܒ̈ܢܝ ܐܝܘܒ ܕܛܡܝܪ̈ܝܢ </l>
                    <l xml:lang="syr">ܘܐ̈ܚܐ ܫܒܥܐ ܕܢܫܝܛܝܢ </l>
                    <l xml:lang="syr">ܥܡ ܣ̈ܛܝܐܐ ܕܪܓܝܡܝܢ </l>
                    <l xml:lang="syr">ܐܝܟܢ ܛܿܠܡ ܐܠܗܐ </l>
                    <l xml:lang="syr">ܗܠܝܢ ܟܠܗܘܢ ܢܨ̈ܚܢܐ </l>
                    
                </lg>
                <lg type="stanza" n="7"> 
                    <l xml:lang="syr">ܘܐܢܬ ܕܘܢ ܒܢܦܫܟ ܕܐܝܟܢܐ </l>
                    <l xml:lang="syr">ܡܨܿܠܐ ܦܘܡܟ ܠܐܠܗܐ </l>
                    <l>܀܀܀</l>
                    <l xml:lang="syr">ܕܦܪܘܥܐ ܗܘ ܐܘ ܛܠܘܡܐ ܗܘ </l>
                    <l xml:lang="syr">ܐܢ ܛܠܘܡܐ ܗܘ ܠܐ ܬܠܐܐ</l>
                    <l xml:lang="syr">ܘܐܢ ܦܪܘܥܐ ܗܘ ܠܐ ܬܫܠܐ </l>
                    <l xml:lang="syr">ܐܓܪܐ ܬܒܿܥ ܕܐܓܝܪܐ </l>
                    <l xml:lang="syr">ܘܛܿܠܡ ܐܓܪܐ ܕܨܠܘܬܐ </l>
                    
                </lg>
                <lg type="stanza" n="8"> 
                    <l xml:lang="syr">ܘܐܢ ܗܪܟܐ ܠܚܘܕ ܦܿܪܥ ܠܟ </l>
                    <l xml:lang="syr">ܚܪܬܐ ܒܝܫܬܐ ܛܿܝܒ ܠܟ </l>
                    <l xml:lang="syr">ܕܢܘܚܡܐ ܠܐ ܥܿܒܕ ܠܟ </l>
                    <l xml:lang="syr">ܐܦ ܠܐ ܗܪܟܐ ܦܿܪܥ ܠܟ </l>
                    <l xml:lang="syr">ܕܟܠܗ ܥܘܡܪܟ ܬܘܢܢܐ ܗܘ </l>
                    <l xml:lang="syr">ܘܠܐ ܗܘܐ ܡܕܡ ܝܬܝܪ ܐܢܬ </l>
                    <l xml:lang="syr">ܠܐܠܗܐ ܡܢ ܚ̈ܛܝܐ </l>
                    
                </lg>
                <lg type="stanza" n="9"> 
                    <l xml:lang="syr">ܚܠܝܨ ܗܘ ܡܢܟ ܓܿܝܨܐ </l>
                    <l xml:lang="syr">ܘܛܘܠܝܩ ܡܢܟ ܓܢܒܐ </l>
                    <l xml:lang="syr">ܘܡܢܚ ܡܢܟ ܙܢܝܐ </l>
                    <l xml:lang="syr">ܘܚܠܝܡ ܡܢܟ ܚܛܝܐ </l>
                    <l xml:lang="syr">ܘܟܗܝܢ ܡܢܟ ܪܫܝܥܐ </l>
                    <l xml:lang="syr">ܘܐܢ ܢܘܚܡܐ ܠܐ ܗܿܘܐ </l>
                    <l xml:lang="syr">ܣܪܝܩ ܗܘ ܪܗܛܟ ܚܠܫܐ </l>
                    
                </lg>
                <lg type="stanza" n="10"> 
                    <l xml:lang="syr">ܘܐܢ ܒܗܠܝܢ ܚ̈ܝܐ ܚܬܝܪܬ </l>
                    <l xml:lang="syr">ܚܘܝܐ ܚܿܝܐ ܛܒ ܡܢܟ </l>
                    <l xml:lang="syr">ܘܝܬܝܪ ܡܢܟ ܬܢܝܢܐ </l>
                    <l xml:lang="syr">ܚ̈ܠܡܝܢ ܡܢܟ ܚܝ̈ܘܬܐ </l>
                    <l xml:lang="syr">ܘܢܫܪ̈ܐ ܡܢܟ ܩܠܝܠܝܢ </l>
                    <l xml:lang="syr">ܘܐܪ̈ܝܘܬܐ ܥܙܝܙܝܢ</l>
                    <l xml:lang="syr">ܘܡܢܐ ܚܬܝܪ ܐܢܬ ܐܘ ܣܟܠܐ</l>
                    
                </lg>
                <lg type="stanza" n="11"> 
                    <l xml:lang="syr">ܐܫܬܥܐ ܠܢ ܚܠܫ </l>
                    <l xml:lang="syr">ܡܢܘ ܐܓܪܐ ܕܨܠܘܬܟ</l>
                    <l xml:lang="syr">ܘܦܘܪܥܢܗ ܕܨܘܡܟ ܡܢܘ </l>
                    <l xml:lang="syr">ܢܕܪܟ ܬܘܟܐ ܗܘ ܝܬܝܪܐ </l>
                    <l>܀܀܀</l>
                    <l xml:lang="syr">ܐܦ ܩܘܪܒܢܟ ܚܘܣܪܢܐ ܗܘ </l>
                    <l xml:lang="syr">ܘܐܢ ܢܘܚܡܐ ܠܐ ܗܿܘܐ </l>
                    <l xml:lang="syr">ܫܘܝܬ ܗܘ ܒܫܝܘܠ ܥܡ ܟܠܢܫ</l>
                    
                </lg>
                <lg type="stanza" n="12"> 
                    <l xml:lang="syr">ܘܐܢ ܡܢ ܣܝܦܐ ܕܚܿܠ ܐܢܬ </l>
                    <l xml:lang="syr">ܕܠܐ ܬܩܛܘܠ ܕܠܡܐ ܬܐܒܕ</l>
                    <l xml:lang="syr">ܘܠܐ ܬܓܢܘܒ ܕܠܡܐ ܬܐܩܕ </l>
                    <l xml:lang="syr">ܘܗܐ ܕܝܿܢܟ ܒܪ ܐܢܫܐ ܗܘ</l>
                    <l xml:lang="syr">ܘܡܨܐ ܒܫܘܚܕܐ ܩܿܛܐ ܠܟ </l>
                    <l xml:lang="syr">ܐܝܟ ܡܐ ܕܩܛܠ ܙܟ̈ܝܐ </l>
                    <l xml:lang="syr">ܘܗܼܦܟ ܙܿܟܝ ܠܚ̈ܝܒܐ </l>
                    
                </lg>
                <lg type="stanza" n="13"> 
                    <l xml:lang="syr">ܟܕ ܬܘܒ ܩܝܡ ܫܘܐܠܐ </l>
                    <l xml:lang="syr">ܕܐܢ ܡܢ ܐܠܗܐ ܐܝܬܘܗܝ</l>
                    <l xml:lang="syr">ܢܡܘܣܐ ܘܕܝܢܼܐ ܟܐܢܐ </l>
                    <l xml:lang="syr">ܐܝܟܢ ܫܦܪܬ ܠܐܠܗܐ </l>
                    <l xml:lang="syr">ܟܐܢܘܬܐ ܕܡܠܐ ܙܒܢܐ </l>
                    <l xml:lang="syr">ܡܢ ܟܐܢܘܬܐ ܕܠܥܠܡ</l>
                    <l xml:lang="syr">ܐܢ ܗܼܘ ܕܛܿܠܡ ܢܘܚܡܢ </l>
                    
                </lg>
                <lg type="stanza" n="14"> 
                    <l xml:lang="syr">ܐܢ ܓܝܪ ܓܐܢܐ ܗܘ ܐܝܟܢܐ ܛܿܠܡ ܠܢ ܒܬܪ ܡܘܬܢ</l>
                    <l xml:lang="syr">ܙܕܩ̈ܬܢ ܘܨ̈ܠܘܬܢ </l>
                    <l xml:lang="syr">ܘܐܢ ܟܐܢܘܬܐ ܠܐ ܪܚܡܿ</l>
                    <l xml:lang="syr">ܗܕܐ ܕܚܣ ܠܢ ܕܢܗܝܡܢ </l>
                    <l xml:lang="syr">ܐܝܟܢܐ ܘܡܢ ܡܿܢ ܫܿܪܝܘ </l>
                    <l xml:lang="syr">ܢܡ̈ܘܣܐ ܟܐ̈ܢܐ ܒܐܪܥܐ </l>
                    
                </lg>
                <lg type="stanza" n="15"> 
                    <l xml:lang="syr">ܐܢ ܢܐܡܪ ܡܢ ܒܪ ܐܢܫܐ </l>
                    <l xml:lang="syr">ܘܟܐܝܢ ܗܘ ܟܝ ܡܢ ܒܪܘܝܗ </l>
                    <l xml:lang="syr">ܗܕܐ ܕܚܼܪܡ ܕܢܡܠܠ </l>
                    <l xml:lang="syr">ܕܗܐ ܒܪܢܫܐ ܥܘܠܐ </l>
                    <l xml:lang="syr">ܒܫܘܚܕܐ ܫܿܪܐ ܢܡ̈ܘܣܐ </l>
                    <l xml:lang="syr">ܘܐܢ ܡܢ ܚܠܩܐ ܗܘ ܢܡܘܣܐ </l>
                    <l xml:lang="syr">ܬܪ̈ܝܗܘܢ ܫܿܪܝܢ ܠܚ̈ܕܕܐ</l>
                    <l>܀܀܀</l>
                    
                </lg>
                <lg type="stanza" n="16"> 
                    <l xml:lang="syr">ܟܕ ܐܢ ܐܠܗܐ ܟܐܢܐ ܗܘ </l>
                    <l xml:lang="syr">ܠܐ ܡܨܐ ܕܢܩܝܡ ܒܝܬ ܝܠܕܐ </l>
                    <l xml:lang="syr">ܕܡܢܗ ܗܘܝܢ ܥܘ̈ܠܐ </l>
                    <l xml:lang="syr">ܘܡܛܠ ܕܐܠܗܐ ܛܿܒܐ ܗܘ </l>
                    <l xml:lang="syr">ܠܐ ܡܨܐ ܣܿܢܐ ܢܘܚܡܐ </l>
                    <l xml:lang="syr">ܕܒܝܫܐ ܗܝ ܣܿܓܝ ܕܢܬܛܠܡܘܢ </l>
                    <l xml:lang="syr">ܙܕܝܩ̈ܐ ܕܡܝܬ ܥܠ ܐ̈ܦܘܗܝ</l>
                    
                </lg>
                <lg type="stanza" n="17"> 
                    <l xml:lang="syr">ܡܢ ܟܠ ܓܒ̈ܝܢ ܐܬܚܒܫܬ </l>
                    <l xml:lang="syr">ܘܐܦ ܐܬܕܪܫܬ ܘܐܬܚܙܝܬ</l>
                    <l xml:lang="syr">ܕܢܘܚܡܐ ܠܡ̈ܝܬܐ ܗܿܘܐ </l>
                    <l xml:lang="syr">ܘܡܢܐ ܬܚܣܪ ܐܘ ܣܟܠܐ</l>
                    <l xml:lang="syr">ܐܢ ܗܼܘ ܕܗܿܘܐ ܢܘܚܡܐ </l>
                    <l xml:lang="syr">ܕܗܠܝܢ ܕܐܝܩܐ ܢܛܿܪ ܐܢܬ</l>
                    <l xml:lang="syr">ܗܝܟܢ ܕܒܗܝܢ ܡܡܠܟ ܐܢܬ</l>
                    
                </lg>
                <lg type="stanza" n="18"> 
                    <l xml:lang="syr">ܕܐܝܠܝܢ ܕܡܓܢ ܢܛܿܪ ܐܢܬ </l>
                    <l xml:lang="syr">ܡܛܠ ܣܝܦܐ ܘܣܪ̈ܩܐ </l>
                    <l xml:lang="syr">ܛܪ ܐܢܝܢ ܡܛܠ ܣܒܪܐ </l>
                    <l xml:lang="syr">ܘܐܿܝܕܐ ܕܡܛܠ ܩܘܠܣܐ </l>
                    <l xml:lang="syr">ܬܗܘܐ ܡܛܠ ܢܘܚܡܐ </l>
                    <l xml:lang="syr">ܚܠܦ ܡܘܬܐ ܕܦܣܩ ܣܒܪܐ </l>
                    <l xml:lang="syr">ܡܘܬ ܡܘܬܐ ܕܡܠܐ ܣܒܪܐ </l>
                    
                </lg>
                <lg type="stanza" n="19"> 
                    <l xml:lang="syr">ܒܢܘܪܐ ܐܘܩܕܘ ܢܨ̈ܝܚܐ </l>
                    <l xml:lang="syr">ܒܡ̈ܝܐ ܚܿܢܩܘ ܠܬܪ̈ܝܨܐ </l>
                    <l xml:lang="syr">ܒܣ̈ܝܦܐ ܢܟܣܘ ܠܐܒ̈ܝܠܐ </l>
                    <l xml:lang="syr">ܘܒܟܐ̈ܦܐ ܪܓܡܼܘ ܠܢܒ̈ܝܐ </l>
                    <l xml:lang="syr">ܘܒܙܩ̈ܝܦܐ ܬܠܘ ܠܫ̈ܠܝܚܐ </l>
                    <l xml:lang="syr">ܘܐܝܟܢ ܟܝ ܛܿܠܡ ܟܐܢܐ </l>
                    <l xml:lang="syr">ܪܒܘ ܪ̈ܒܘܢ ܓܘ̈ܥܠܢܝܢ </l>
                    
                    <l>܀܀܀</l>
                    
                </lg>
