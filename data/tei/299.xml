        <body>
            <div type="title" n="45">
                <ab xml:lang="en">Hymn XLV</ab>
            </div>
            <div type="body">
                <lg type="superscript">
                    <l xml:lang="syr">ܓ ܒܪ ܩܠܗ</l>
                </lg>
                <lg type="stanza" n="1"> 
                    <l xml:lang="syr">ܐܢ ܐܟܠܐ ܢܦܫܐ </l>
                    <l xml:lang="syr">ܘܨܝܡܐ ܘܡܬܦܪܥܐ </l>
                    <l xml:lang="syr">ܙܕܩ ܕܐܦ ܦܓܪܐ </l>
                    <l xml:lang="syr">ܢܬܦܪܥ ܕܨܡ ܥܡܗܿ</l>
                    <l xml:lang="syr">ܘܐܢ ܗܼܘ ܕܝܕܝܥܐ ܗܝ </l>
                    <l xml:lang="syr">ܕܦܓܪܐ ܗܘ ܐܟܠ ܘܐܦ ܨܡ</l>
                    <l xml:lang="syr">ܡܿܟܣ ܠܛ̈ܥܝܐ </l>
                    <l>܀܀܀</l>
                    <l xml:lang="syr">ܟܡܐ ܛܥܘ</l>
                    <l xml:lang="syr">ܕܢܦܫܐ ܕܠܐ ܨܡܬ </l>
                    <l xml:lang="syr">ܗܼܝ ܬܣܒܝܘܗܝ ܠܐܓܪܐ </l>
                    <l xml:lang="syr">ܘܢܗܘܐ ܕܠܐ ܐܓܪܐ </l>
                    <l xml:lang="syr">ܦܓܪܐ ܕܨܡ ܘܠܐܝ</l>
                    
                </lg>
                
                <lg type="refrain">
                    <l xml:lang="syr">ܥܘܢܝܬܐ : ܢܩܒܠ ܒܢܘܚܡܟ ܦܓܪܐ ܠܒܪܬ ܙܘܓܗ</l>
                    
                </lg>
                <lg type="stanza" n="2"> 
                    <l xml:lang="syr">ܐܠܝܨܐ ܗܝ ܠܡ ܐܦ ܗܼܝ </l>
                    <l xml:lang="syr">ܒܨܘܡ̈ܘܗܝ ܘܒܐܘܠܨ̈ܢܘܗܝ</l>
                    <l xml:lang="syr">ܘܐܢ ܠܗܿ ܛܐܒܘ ܚ̈ܫܐ </l>
                    <l xml:lang="syr">ܫܐ̈ܝܠܐ ܕܡܢ ܨܐܕܘܗܝ</l>
                    <l xml:lang="syr">ܟܡܐ ܟܝܬ ܢܚܐ ܗܘ </l>
                    <l xml:lang="syr">ܒܢ̈ܓܕܐ ܕܗܕܡ̈ܘܗܝ </l>
                    <l xml:lang="syr">ܗܿܝ ܓܝܪ ܥܘܠܐ ܗܝ </l>
                    <l xml:lang="syr">ܠܡܫܡܥܗܿ</l>
                    <l xml:lang="syr">ܕܐܢ ܗܼܘ ܕܗܝ ܝܪܒܬ </l>
                    <l xml:lang="syr">ܒܗܘܦܐ ܕܢܨ̈ܚܢܘܗܝ</l>
                    <l xml:lang="syr">ܟܡܐ ܟܝܬ ܢܐܪܒ ܗܼܘ </l>
                    <l xml:lang="syr">ܒܐܓܪܐ ܕܡܪ̈ܗܛܘܗܝ</l>
                    
                </lg>
                <lg type="stanza" n="3"> 
                    <l xml:lang="syr">ܣܢܝܐ ܗܝ ܕܗܿܘ ܥܡܿܠ </l>
                    <l xml:lang="syr">ܘܗܼܝ ܢܣܒܐ ܐܓܪܐ </l>
                    <l xml:lang="syr">ܣܢܝܐ ܗܝ ܕܠܗ ܢܘܪܐ </l>
                    <l xml:lang="syr">ܘܠܗܿ ܡܘܕܝܢܘܬܐ </l>
                    <l xml:lang="syr">ܣܟܠܐ ܗܝ ܕܠܗ ܣܝܦܐ </l>
                    <l xml:lang="syr">ܘܠܗܿ ܬܘܒ ܟܠܝܠ ܚ̈ܝܐ </l>
                    <l xml:lang="syr">ܗܢܐ ܩܛܝܠ ܡܛܠ </l>
                    <l xml:lang="syr">ܒܪܝܗ</l>
                    <l xml:lang="syr">ܚܝ̈ܘܗܝ ܬܢܢ ܐܘܒܕ </l>
                    <l xml:lang="syr">ܘܢܘܚܡܬܗ ܕܠܗܠ</l>
                    <l xml:lang="syr">ܐܘ ܡܐ ܦܬܝܠ ܟܠܗ </l>
                    <l xml:lang="syr">ܫܪܒܗܘܢ ܕܛܥ̈ܝܐ </l>
                    
                </lg>
                <lg type="stanza" n="4"> 
                    <l xml:lang="syr">ܕܦܓܪܐ ܗܘ ܓܝܪ ܟܦܢܐ </l>
                    <l xml:lang="syr">ܫܗܪܐ ܘܐܦ ܨܗܝܐ </l>
                    <l xml:lang="syr">ܡܠܘܐܐ ܕܒܠܥܕܘܗܝ </l>
                    <l xml:lang="syr">ܠܐ ܥܬܪܐ ܢܦܫܐ </l>
                    <l xml:lang="syr">ܛܠܡܬܗ ܠܬܓܪܐ </l>
                    <l xml:lang="syr">ܩܪܢܐ ܘܪܒܝܬܐ </l>
                    <l xml:lang="syr">ܕܗܐ ܥܬܪܐ ܒܝܘܬܪܢ </l>
                    <l xml:lang="syr">ܟܟܪ̈ܘܗܝ</l>
                    <l xml:lang="syr">ܘܚܠܦ ܝܘܬܪ̈ܢܘܗܝ </l>
                    <l xml:lang="syr">ܫܛܪܐ ܕܚܘܒ̈ܬܐ </l>
                    <l xml:lang="syr">ܟܬܒܘ ܒܬܪܗ ܣ̈ܟܠܐ </l>
                    <l xml:lang="syr">ܕܥܒܕܘܗܝ ܕܠܐ ܢܘܚܡ</l>
                    
                </lg>
                <lg type="stanza" n="5"> 
                    <l xml:lang="syr">ܥܘܠܐ ܗܘ ܕܗܿܘ ܟܦܼܢ</l>
                    <l xml:lang="syr">ܘܡܫܬܒܗܪܐ ܒܨܘܡܗ </l>
                    <l xml:lang="syr">ܥܘܠܐ ܗܘ ܕܗܿܘ ܨܐܝܦ </l>
                    <l xml:lang="syr">ܘܡܬܩܠܣܐ ܒܨܗܝܗ</l>
                    <l xml:lang="syr">ܐܓܪ ܦܥ̈ܠܘܗܝ ܫܩܠܬ </l>
                    <l xml:lang="syr">ܘܠܗ ܫܡܪܬ ܫܒܩܬ</l>
                    <l xml:lang="syr">ܘܐܝܟ ܕܐܡܪܝܢ ܒܢܘܪܐ ܗܘ ܡܬܦܪܫ</l>
                    <l xml:lang="syr">ܛܠܡܘܗܝ ܠܐܟܪܐ </l>
                    <l xml:lang="syr">ܕܢܫܦܘ ܫܩܼܠܘ ܙܪ̈ܥܘܗܝ</l>
                    <l xml:lang="syr">ܘܠܗ ܐܘܪܬܘܗܝ ܥܘܪܐ </l>
                    <l xml:lang="syr">ܘܬܒܢܐ ܕܢܐܩܕ ܒܗ</l>
                    
                </lg>
                <lg type="stanza" n="6"> 
                    <l xml:lang="syr">ܟܐܢܐ ܗܝ ܕܗܼܘ ܒܘܫܒܚܐ </l>
                    <l xml:lang="syr">ܘܐܦ ܗܝ ܒܬܫܒܘܚܬܐ </l>
                    <l xml:lang="syr">ܟܐܢܐ ܗܝ ܕܗܼܘ ܒܚ̈ܝܐ </l>
                    <l xml:lang="syr">ܘܐܦ ܗܼܝ ܒܚܝܘܬܐ </l>
                    <l xml:lang="syr">ܟܐܢܐ ܗܝ ܕܗܼܘ ܒܥܕܢ </l>
                    <l xml:lang="syr">ܐܦ ܗܼܝ ܒܦܪܕܝܣܐ </l>
                    <l xml:lang="syr">ܗܿܝ ܓܝܪ ܥܘܠܐ ܗܝ </l>
                    <l xml:lang="syr">ܕܐܡܪܝܢ</l>
                    <l xml:lang="syr">ܕܦܠܓܗ ܕܒܪ ܐܢܫܐ </l>
                    <l xml:lang="syr">ܢܚܐ ܒܡܠܟܘܬܐ </l>
                    <l xml:lang="syr">ܘܦܠܓܗ ܕܠܐ ܣܟܠܘ </l>
                    <l xml:lang="syr">ܢܫܬܕܐ ܠܫܘܢܩܐ </l>
                    
                    <l>܀܀܀</l>
                </lg>
                <lg type="stanza" n="7"> 
                    <l xml:lang="syr">ܥܘܝܪܝܢ ܒ̈ܢܝ ܛܘܥܝܝ </l>
                    <l xml:lang="syr">ܠܡܚܪ ܒܐܝܓܘܢܐ </l>
                    <l xml:lang="syr">ܕܐܢ ܗܼܘ ܕܙܟܘܬܐ ܗܝ </l>
                    <l xml:lang="syr">ܩܪܐܘܗܿ ܒܫܡ ܢܦܫܐ </l>
                    <l xml:lang="syr">ܘܐܢ ܗܼܘ ܕܟܘܐܪܐ ܗܘ </l>
                    <l xml:lang="syr">ܬܠܐܘܗܝ ܒܫܡ ܦܓܪܐ </l>
                    <l xml:lang="syr">ܘܐܦ ܠܐ ܐܝܟ ܚ̈ܙܝܐ </l>
                    <l xml:lang="syr">ܕܥܠܡܐ </l>
                    <l xml:lang="syr">ܕܒܙܒܢ ܠܕܒܘܪܐ </l>
                    <l xml:lang="syr">ܡܘܪܒܝܢ ܕܡܪܟܒܬܐ </l>
                    <l xml:lang="syr">ܘܒܙܒܢ ܠܪ̈ܟܫܐ ܬܘܒ </l>
                    <l xml:lang="syr">ܡܘܪܒܝܢ ܕܐܬܢܨܿܚܘ </l>
                    
                    
                </lg>
                <lg type="stanza" n="8"> 
                    <l xml:lang="syr">ܐܢ ܗܼܘ ܕܡܢ ܐܝܗܬܝܐ ܗܝ </l>
                    <l xml:lang="syr">ܫ̈ܦܝܐ ܘܫܦܝܪ̈ܐ </l>
                    <l xml:lang="syr">ܦܓܪܐ ܗܘ ܣܢܐ ܡܕܝܢ </l>
                    <l xml:lang="syr">ܘܠܐܐ ܕܢܨܛܒܬ </l>
                    <l xml:lang="syr">ܐܢ ܐܣܬܢܝ ܘܝ ܠܗ </l>
                    <l xml:lang="syr">ܘܐܢ ܫܦܪ ܬܪ̈ܝܢ ܘܝ̈ܝܢ</l>
                    <l xml:lang="syr">ܘܟܕ ܗܼܘ ܛܥܝܢ ܝܘܩܪܐ </l>
                    <l xml:lang="syr">ܕܪܓܬܐ </l>
                    <l xml:lang="syr">ܕܢܦܫܐ ܗܝ ܒܬܘܠܘܬܐ </l>
                    <l xml:lang="syr">ܘܐܪܓܐ ܕܢܟܦܘܬܐ </l>
                    <l xml:lang="syr">ܐܬܝܐ ܘܪܡ ܩܠܗܿ </l>
                    <l xml:lang="syr">ܒܚܕܥܣܪ ܫ̈ܥܝܢ </l>
                    
                </lg>
                <lg type="stanza" n="9"> 
                    <l xml:lang="syr">ܘܐܢ ܗܼ ܕܕܝܠܗܿ ܗܝ </l>
                    <l xml:lang="syr">ܪܓܬܐ ܘܝܥܢܘܬܐ </l>
                    <l xml:lang="syr">ܦܓܪܐ ܩܒܝܠ ܡܢܗܿ </l>
                    <l xml:lang="syr">ܕܐܬܚܢܩ ܒܣ̈ܢܝܬܗܿ</l>
                    <l xml:lang="syr">ܐܢ ܟܝܬ ܗܼܝ ܫܓܝܫܐ </l>
                    <l xml:lang="syr">ܦܚܙܐ ܘܙܠܝܠܐ</l>
                    <l xml:lang="syr">ܠܡܐ ܐܬܥܫܩ ܦܓܪܐ </l>
                    <l xml:lang="syr">ܒܪܓܬܗܿ</l>
                    <l>܀܀܀</l>
                    <l xml:lang="syr">ܝܘܣܦ ܥܫܝܩ ܘܫܠܐ </l>
                    <l xml:lang="syr">ܘܩܠ ܡܪܬܗ ܥܙܝܙ</l>
                    <l xml:lang="syr">ܠܢܒܘܬ ܡܼܛܐ ܝܓܪܐ </l>
                    <l xml:lang="syr">ܘܠܐܝܙܒܠ ܟܪܡܐ </l>
                    
                </lg>
                <lg type="stanza" n="10"> 
                    <l xml:lang="syr">ܘܐܝܟܢ ܡܼܨܐ ܦܓܪܐ </l>
                    <l xml:lang="syr">ܡܬܫܝܓ ܒܟܠ ܦܘܪ̈ܣܝܢ </l>
                    <l xml:lang="syr">ܐܢ ܗܼܘ ܕܡܦܠܦܠ ܗܘ </l>
                    <l xml:lang="syr">(Missing Letter "c")ܬܛܪܐ ܕܐܝܬܘܬܗ</l>
                    <l xml:lang="syr">ܘܐܝܟܢ ܕܠܚ ܫܦܝܐ </l>
                    <l xml:lang="syr">ܟܝܢܗܿ ܕܗܿܝ ܢܦܫܐ </l>
                    <l xml:lang="syr">ܠܐ ܓܝܪ ܡܨܐ ܦܓܪܐ </l>
                    <l xml:lang="syr">ܕܢܕܠܚܝܗܿ</l>
                    <l xml:lang="syr">ܕܠܐ ܡܬܕܠܚ ܢܘܗܪܐ </l>
                    <l xml:lang="syr">ܡܢܗܘܢ ܕܛ̈ܠܠܐ </l>
                    <l xml:lang="syr">ܐܠܐ ܕܡܪܩ ܠܗ </l>
                    <l xml:lang="syr">ܒܕܢܚܗ ܠܛܠܠܐ</l>
                    
                </lg>
                <lg type="stanza" n="11"> 
                    <l xml:lang="syr">ܣܟ̈ܠܐ ܕܗܦܟܝܝܢ </l>
                    <l xml:lang="syr">ܗܦܟܘܗܿ ܠܫܘܥܝܬܐ </l>
                    <l xml:lang="syr">ܕܚܫܟܐ ܗܘ ܠܡ ܐܚܨܦ </l>
                    <l xml:lang="syr">ܘܕܠܚܗ ܠܢܗܝܪܐ </l>
                    <l xml:lang="syr">ܦܫܝܩܐ ܗܝ ܕܢܬܟܣܣܘܢ </l>
                    <l xml:lang="syr">ܡܢ ܣܗ̈ܕܐ ܕܐܝܬܝܘ </l>
                    <l xml:lang="syr">ܬܪ̈ܝܗܘܢ ܓܝܪ ܥܡܢ </l>
                    <l xml:lang="syr">ܡܗܠܟܝܢ </l>
                    <l xml:lang="syr">ܘܠܐ ܡܨܛܒܥ ܡܡܬܘܡ </l>
                    <l xml:lang="syr">ܢܘܗܪܐ ܒܚܫܘܟܐ </l>
                    <l xml:lang="syr">ܐܠܐ ܕܨܿܒܥ ܠܗ </l>
                    <l xml:lang="syr">ܒܕܢܚܗ ܠܐܘܟܡܐ </l>
                    
                </lg>
                <lg type="stanza" n="12"> 
                    <l xml:lang="syr">ܚܕܐ ܗܝ ܓܝܪ ܡܢ ܬܪ̈ܬܝܢ </l>
                    <l xml:lang="syr">ܢܓܒܘܢ ܒ̈ܢܝ ܛܘܥܝܝ</l>
                    <l xml:lang="syr">ܕܐܠܐ ܡܨܐ ܦܓܪܐ </l>
                    <l>܀܀܀</l>
                    <l xml:lang="syr">ܕܢܫܦܪ ܘܢܬܚܠܠ</l>
                    <l xml:lang="syr">ܚܢܝܩܐ ܗܝ ܒܣܝܢܗ </l>
                    <l xml:lang="syr">ܢܦܫܐ ܕܫܪܝܐ ܒܗ </l>
                    <l xml:lang="syr">ܘܐܝܩܐ ܗܘ ܡܫܬܚܩܐ </l>
                    <l xml:lang="syr">ܕܬܬܡܪܩ </l>
                    <l xml:lang="syr">ܘܐܢ ܕܝܢ ܗܼܘ ܦܓܪܐ </l>
                    <l xml:lang="syr">ܟܝܢܐ ܗܘ ܕܡܫܬܦܐ </l>
                    <l xml:lang="syr">ܒܝܫܐ ܕܠܚ ܐܢܘܢ </l>
                    <l xml:lang="syr">ܕܥܒܕܘܗܝ ܕܠܐ ܢܘܚܡ</l>
                    
                </lg>
                <lg type="stanza" n="13"> 
                    <l xml:lang="syr">ܢܫܐܠ ܠܟܦܘܪ̈ܐ </l>
                    <l xml:lang="syr">ܕܡܕܡ ܕܡܝܬܐ ܗܘܐ </l>
                    <l xml:lang="syr">ܡܢܘ ܪܟܒ ܗܘܐ </l>
                    <l xml:lang="syr">ܥܠܬܐ ܠܚܝܘܬܗ </l>
                    <l xml:lang="syr">ܒܗܿܝ ܥܠܬܐ ܕܚܝܐ </l>
                    <l xml:lang="syr">ܡܨܐ ܗܘܐ ܕܢܚܐ ܒܗܿ</l>
                    <l xml:lang="syr">ܘܐܢ ܕܝܢ ܕܫܪܘܬܝܢ </l>
                    <l xml:lang="syr">ܐܚܝܗ</l>
                    <l xml:lang="syr">ܝܬܝܪ ܡܢܚܿܡ ܠܗ </l>
                    <l xml:lang="syr">ܕܐܛܥܡ ܒܚܝܘܬܐ </l>
                    <l xml:lang="syr">ܢܒܗܬܘܢ ܒܥ̈ܛܠܬܐ </l>
                    <l xml:lang="syr">ܟܦܪ̈ܝ ܒܕܦܫ̈ܝܩܢ</l>
                    
                </lg>
                <lg type="stanza" n="14"> 
                    <l xml:lang="syr">ܗܿܘ ܕܐܚܝܗ ܩܠܝܠ </l>
                    <l xml:lang="syr">ܠܐ ܡܚܝܠ ܕܐܦ ܣܿܓܝ</l>
                    <l xml:lang="syr">ܘܕܩܪܝܗܝ ܡܢ ܩܒܪܐ </l>
                    <l xml:lang="syr">ܠܐ ܠܐܝ ܕܬܘܒ ܢܩܪܝܘܗܝ </l>
                    <l xml:lang="syr">ܘܕܛܒ ܒܘܩܢܗ </l>
                    <l xml:lang="syr">ܟܡܐ ܟܐܝܢ ܒܢܘܚܡܗ </l>
                    <l xml:lang="syr">ܕܐܢ ܐܚܝܗ ܡܓܢ </l>
                    <l xml:lang="syr">ܡܢ ܩܕܝܡ </l>
                    <l xml:lang="syr">ܟܡܐ ܟܝ ܡܢܚܿܡ ܠܗ </l>
                    <l xml:lang="syr">ܟܐܢܐ ܕܦܿܪܥ ܠܗ </l>
                    <l xml:lang="syr">ܐܬܟܣܣܘ ܒܛܝܒܘܬܐ </l>
                    <l xml:lang="syr">ܓ̈ܠܙܝ ܠܟܐܢܘܬܐ</l>
                    
                    <l>܀܀܀</l>
                </lg>
                <lg type="stanza" n="15"> 
                    <l xml:lang="syr">ܐܢ ܕܝܢ ܗܼܘ ܦܓܪܐ </l>
                    <l xml:lang="syr">ܟܝܢܐ ܗܘ ܕܠܐ ܡܫܟܚ </l>
                    <l xml:lang="syr">ܕܢܐܬܐ ܠܢܘܚܡܐ </l>
                    <l xml:lang="syr">ܢܫܐܠ ܩܕܡ ܗܕܐ </l>
                    <l xml:lang="syr">ܕܐܝܟܢ ܗܘܐ ܦܘܪܣܐ </l>
                    <l xml:lang="syr">ܕܢܚܐ ܒܝܕ ܢܦܫܐ </l>
                    <l xml:lang="syr">ܗܿܘ ܓܝܪ ܨܒܝܢܐ </l>
                    <l xml:lang="syr">ܕܐܚܝܗ </l>
                    <l xml:lang="syr">ܐܝܟܢ ܣܦܩ ܐܚܝ </l>
                    <l xml:lang="syr">ܠܡܕܡ ܕܠܐ ܚܿܝܐ </l>
                    <l xml:lang="syr">ܐܙܕܟܝܘ ܒܚܝܘܬܗ </l>
                    <l xml:lang="syr">ܟܦܪ̈ܝ ܒܢܘܚܡܗ</l>
                    
                </lg>
                <lg type="stanza" n="16"> 
                    <l xml:lang="syr">ܐܦ ܗܿܘ ܓܿܝܣܐ </l>
                    <l xml:lang="syr">ܕܐܪܓܫ ܒܣܝܡ̈ܬܟ</l>
                    <l xml:lang="syr">ܣܢܐܓܪܐ ܗܘܐ ܗܘܐ ܠܗ </l>
                    <l xml:lang="syr">ܠܫܬܩܟ ܡܿܙܟܐ ܟܠ</l>
                    <l xml:lang="syr">ܘܥܬܪ ܒܝܡܝܢܐ </l>
                    <l xml:lang="syr">ܕܐܟܣܗܿ ܠܣܡܠܐ </l>
                    <l xml:lang="syr">ܚܘܢ ܡܪܝ ܠܚܛܝܘܬܝ </l>
                    <l xml:lang="syr">ܕܗܡܢܬ</l>
                    <l xml:lang="syr">ܘܐܟܣܬ ܠܟܦܘܪ̈ܐ </l>
                    <l xml:lang="syr">ܩ̈ܝܡܝ ܒܣܡܠܐ </l>
                    <l xml:lang="syr">ܗܢܘ ܐܓܪ ܡ̈ܠܝ </l>
                    <l xml:lang="syr">ܕܠܐܬܬܥܢܝ ܚܿܘܒܝ</l>
                    
                    
                </lg>
            </div>
        </body>
    </text>
</TEI>
